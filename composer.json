{
    "name": "shortlist-digital/agreable-instant-articles-plugin",
    "type": "wordpress-plugin",
    "description": "Wordpress plugin for Shortlist Media.",
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/shortlist-digital/framework"
        }
    ],
    "require": {
        "shortlist-digital/framework": "dev-master",
        "illuminate/contracts": "5.3.x",
        "nategood/httpful": "dev-master",
        "guzzlehttp/guzzle": "~6.0",
        "jimmiw/php-time-ago": "^0.4.10",
        "facebook/facebook-instant-articles-sdk-php": "1.*",
        "sunra/php-simple-html-dom-parser": "1.5.2"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "AgreableInstantArticlesPlugin\\": "app/"
        }
    }
}
